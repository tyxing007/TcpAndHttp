<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.excel.dao.ConfigDao" >

	<select id="getIndustry" resultType="com.excel.vo.SysDicVO">
		SELECT id,`value`,screen_name FROM `sys_dict` WHERE id = 'industry'
	</select>
	
	<select id="getCity" resultType="com.excel.vo.CityVO">
		SELECT  `id`,  `name` FROM `city`
	</select>
</mapper>